{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@logue/smfplayer",
  "description": "smfplayer.js is JavaScript based Standard Midi Player for WebMidiLink based synthesizer.",
  "type": "module",
  "author": {
    "name": "imaya / GREE Inc.",
    "url": "https://github.com/gree"
  },
  "contributors": [
    {
      "name": "Logue",
      "email": "logue@hotmail.co.jp",
      "url": "https://logue.dev"
    }
  ],
  "version": "0.4.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logue/smfplayer.js.git"
  },
  "keywords": [
    "Maple Story 2",
    "WebMidiLink",
    "mabinogi",
    "midi player",
    "midi",
    "mml",
    "ms2mml",
    "Music Macro Language",
    "sequencer",
    "smf",
    "wml",
    "webaudio"
  ],
  "bugs": {
    "url": "https://github.com/logue/smfplayer.js/issues"
  },
  "homepage": "https://logue.dev/smfplayer.js/",
  "files": [
    "dist"
  ],
  "main": "dist/smfplayer.umd.js",
  "module": "dist/smfplayer.es.js",
  "exports": {
    ".": {
      "import": "./dist/smfplayer.es.js",
      "require": "./dist/smfplayer.umd.js"
    }
  },
  "engines": {
    "node": ">=16.17.1",
    "yarn": ">=1.22.10"
  },
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "dev": "vite",
    "dev:doc": "vite --config vite.config.docs.js",
    "clean": "rimraf node_modules/.vite",
    "build": "vite build",
    "build:docs": "vite build --config vite.config.docs.js",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "preview": "vite preview --config vite.config.docs.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "ini": "^3.0.1"
  },
  "devDependencies": {
    "@logue/sf2synth": "^0.4.9",
    "@popperjs/core": "^2.11.6",
    "@rollup/plugin-inject": "^4.0.4",
    "bootstrap": "^5.2.2",
    "bootstrap-icons": "^1.9.1",
    "buffer": "^6.0.3",
    "dseg": "^0.46.0",
    "encoding-japanese": "^2.0.0",
    "eslint": "^8.24.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "query-string": "^7.1.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass": "^1.55.0",
    "streamsaver": "^2.0.6",
    "vite": "^3.1.4",
    "vite-plugin-banner": "^0.5.0",
    "vite-plugin-checker": "^0.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,htm,html,md}": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "prettier": "^2.7.1"
  }
}
