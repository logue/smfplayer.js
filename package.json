{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@logue/smfplayer",
  "version": "0.4.0",
  "description": "smfplayer.js is JavaScript based Standard Midi Player for WebMidiLink based synthesizer.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logue/smfplayer.js.git"
  },
  "keywords": [
    "Maple Story 2",
    "WebMidiLink",
    "mabinogi",
    "midi player",
    "midi",
    "mml",
    "ms2mml",
    "Music Macro Language",
    "sequencer",
    "smf",
    "wml",
    "webaudio"
  ],
  "author": {
    "name": "imaya / GREE Inc.",
    "url": "https://github.com/gree"
  },
  "contributors": [
    {
      "name": "Logue",
      "email": "logue@hotmail.co.jp",
      "url": "https://logue.dev"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/logue/smfplayer.js/issues"
  },
  "homepage": "https://logue.dev/smfplayer.js/",
  "files": [
    "dist"
  ],
  "main": "./dist/smf.umd.js",
  "module": "./dist/smf.es.js",
  "exports": {
    ".": {
      "import": "./dist/smf.es.js",
      "require": "./dist/smf.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf node_modules/.vite",
    "build": "vite build",
    "build:docs": "vite build --config vite.config.docs.js",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "preview": "vite preview --config vite.config.docs.js",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16.16.0",
    "yarn": ">=1.22.4"
  },
  "packageManager": "yarn@3.2.1",
  "dependencies": {
    "ini": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "vite": "^2.9.14",
    "vite-plugin-banner": "^0.3.0",
    "vite-plugin-checker": "^0.4.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,htm,html,md}": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "prettier": "^2.7.1"
  }
}
