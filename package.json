{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@logue/smfplayer",
  "version": "0.3.7",
  "description": "smfplayer.js is JavaScript based Standard Midi Player for WebMidiLink based synthesizer.",
  "scripts": {
    "dev": "webpack --mode development",
    "prod": "webpack --mode production",
    "serve": "webpack-dev-server --color --mode development",
    "lint": "eslint --cache . --fix && prettier . --write",
    "prepare": "husky install"
  },
  "bin": {
    "smf.parser.min.js": "bin/smf.parser.min.js",
    "smf.player.min.js": "bin/smf.player.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logue/smfplayer.js.git"
  },
  "keywords": [
    "Maple Story 2",
    "WebMidiLink",
    "mabinogi",
    "midi player",
    "midi",
    "mml",
    "ms2mml",
    "Music Macro Language",
    "sequencer",
    "smf",
    "wml",
    "webaudio"
  ],
  "author": {
    "name": "imaya / GREE Inc.",
    "url": "https://github.com/gree"
  },
  "contributors": [
    {
      "name": "Logue",
      "email": "logue@hotmail.co.jp",
      "url": "https://logue.dev"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/logue/smfplayer.js/issues"
  },
  "homepage": "https://logue.dev/smfplayer.js",
  "dependencies": {
    "ini": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "eslint": "^8.10.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier-plugin-jsdoc": "^0.3.30",
    "prettier-plugin-md-nocjsp": "^1.2.0",
    "prettier-plugin-sort-imports": "^1.6.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
