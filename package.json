{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@logue/smfplayer",
  "description": "smfplayer.js is JavaScript based Standard Midi Player for WebMidiLink based synthesizer.",
  "type": "module",
  "author": {
    "name": "imaya / GREE Inc.",
    "url": "https://github.com/gree"
  },
  "contributors": [
    {
      "name": "Logue",
      "email": "logue@hotmail.co.jp",
      "url": "https://logue.dev"
    }
  ],
  "version": "0.5.0-beta.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logue/smfplayer.js.git"
  },
  "keywords": [
    "Maple Story 2",
    "WebMidiLink",
    "mabinogi",
    "midi player",
    "midi",
    "mml",
    "ms2mml",
    "Music Macro Language",
    "sequencer",
    "smf",
    "wml",
    "webaudio"
  ],
  "bugs": {
    "url": "https://github.com/logue/smfplayer.js/issues"
  },
  "homepage": "https://logue.dev/smfplayer.js/",
  "files": [
    "dist"
  ],
  "main": "dist/smfplayer.umd.js",
  "module": "dist/smfplayer.es.js",
  "exports": {
    ".": {
      "import": "./dist/smfplayer.es.js",
      "require": "./dist/smfplayer.umd.js"
    }
  },
  "engines": {
    "node": ">=18.12.1",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "dev": "vite",
    "dev:doc": "vite --mode=docs",
    "clean": "rimraf node_modules/.vite",
    "build": "vite build",
    "build:docs": "vite build --mode=docs",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "preview": "vite preview--mode=docs",
    "prepare": "husky install"
  },
  "dependencies": {
    "ini": "^3.0.1"
  },
  "devDependencies": {
    "@logue/smfplayer": "^0.4.3",
    "@popperjs/core": "^2.11.6",
    "@types/ini": "^1.3.31",
    "bootstrap": "5.3.0-alpha1",
    "bootstrap-icons": "^1.10.3",
    "buffer": "^6.0.3",
    "dseg": "^0.46.0",
    "encoding-japanese": "^2.0.0",
    "eslint": "^8.33.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "query-string": "^8.1.0",
    "rimraf": "^4.1.2",
    "sass": "^1.57.1",
    "streamsaver": "^2.0.6",
    "vite": "^4.0.4",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-checker": "^0.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,htm,html,md}": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": "^2.2.3"
  }
}
